{
  "expo": {
    "name": "Ciko",
    "slug": "ciko",
    "version": "1.0.4",
    "newArchEnabled": true,
    "orientation": "portrait",
    "icon": "./assets/platform/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "backgroundColor": "#f9d8ec",
    "androidStatusBar": {
      "translucent": true
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "icon":{
        "light": "./assets/platform/icon-ios.png"
      },
      "supportsTablet": false,
      "bundleIdentifier": "com.wervin.ciko",
      "config": {
        "usesNonExemptEncryption": false
      },
      "infoPlist": {
        "CFBundleDevelopmentRegion": "fr-FR"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/platform/icon-android.png"
      },
      "package": "com.wervin.ciko"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#f9d8ec",
          "image": "./assets/platform/splash-icon.png",
          "imageWidth": 200
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "6d2295e6-84eb-4c04-8c94-625857a304a6"
      }
    },
    "owner": "wervin"
  }
}